#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N , first , last , temp;
    scanf("%d", &N);
     temp = N;

    last = N % 10;

    while (temp >= 10) {
        temp = temp / 10;
    }
    first = temp;

    printf("%d %d\n", first, last);
    
    return 0;
}
