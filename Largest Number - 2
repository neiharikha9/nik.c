#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int A =10 , B =20 , C =30;
    scanf("%d %d %d", &A ,&B ,&C);
    int largest = A;
    if(B > largest)
        largest = B;
    if(C > largest)
        largest = C;
    printf("%d", largest);
        
    return 0;
}
